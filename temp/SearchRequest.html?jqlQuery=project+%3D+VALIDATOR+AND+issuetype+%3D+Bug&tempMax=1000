                    </tr>
                </tbody>
    </table>
    <div class="end-of-stable-message"></div>
            </div>
<script>
    // Prevent click events from doing stuff in this view
    AJS.$(function(){
        AJS.$('#printable-content *').unbind('click').click(function(e){ e.preventDefault() }).removeAttr('onclick');
    });
</script>
